<% header "User Profile" %><hr/><% if @restaurants.any? %>	<b>You have created <%= pluralize(@restaurants.count, "restaurant") %></b>	<br/><br/>	<div id="user_restaurants" class="listing_box">	  <table id="user_restaurant_listing" class="listing_table">	  <% @restaurants.each do |restaurant| %>		<%= render partial: "layouts/restaurant_listing", locals: { restaurant: restaurant } %>	  <% end %>	  </table>	  <%= will_paginate @restaurants, param_name: "restaurants_page" %>	</div><% end %><% if @meals.any? %>	<hr/>	<b>You have created <%= pluralize(@meals.count, "meal") %></b>	<br/><br/>	<div id="user_meals" class="listing_box">	  <table id="user_meal_listing" class="listing_table">	  <% @meals.each do |meal| %>		<%= render partial: "layouts/meal_listing", locals: { meal: meal } %>	  <% end %>	  </table>	  <%= will_paginate @meals, param_name: "meals_page"%>	</div><% end %><br/>